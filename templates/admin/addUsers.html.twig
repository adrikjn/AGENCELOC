{% extends "base.html.twig" %}

{% block body %}
<div class="container">
    <h1 class="text-center text-primary my-4">Ajouter Utilisateur</h1>
    {{ form_start(form, {
            novalidate: 'novalidate'
        }) }}
        {{ form_row(form.pseudo)}}
        {{ form_row(form.password)}}
        {{ form_row(form.nom)}}
        {{ form_row(form.prenom)}}
        {{ form_row(form.email)}}
        {# {{ form_row(form.statut|join(', '))}} #}
        {{ form_row(form.civilite)}}
    {{ form_end(form) }}
</div>
{% endblock %}